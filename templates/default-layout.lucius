body {
    padding-top: 50px;
    color: rgb(9, 28, 36);
}

body, h1, h2, h3, h4, .navbar {
    font-family: Lato, Helvetica, Arial, sans-serif;
}

.navbar {
    background-color: #AA2163;

    a {
        color: white;
    }

    .icon-bar {
        border: 1px solid white;
    }

    .nav li {
        margin-left: 5px;
        margin-right: 5px;
    }

    .nav li.active, .nav li:hover, .nav li a:hover {
        background-color: #89215B;
    }
}
